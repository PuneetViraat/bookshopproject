@model IEnumerable<ProjectECommerce.Models.OrderDetail>
<div class="row">
    <div class="col-9">
        <h2 class="text-primary">My Previous Orders</h2>

    </div>
</div>

<div class="container rounded border p-2 m-2">
    <table class="table table-striped table-bordered table-hover">
        <thead>
            <tr>
                <th>Product Name</th>
                <th>Product image</th>
                <th>Count</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <td>@item.Product.Title</td>
                <td>@item.Product.ImageUrl</td>
                <td>@item.Count</td>
                
            }
        </tbody>

    </table>

</div>